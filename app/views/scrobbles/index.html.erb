<h1>Scrobbles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Played At</th>
      <th>Artist</th>
      <th>Track</th>
      <th>Album</th>
    </tr>
  </thead>
  <tbody>
  <% @scrobbles.each do |scrobble| %>
    <tr>
      <td><%= scrobble.user.username %></td>
      <td><%= scrobble.listened_at %></td>
      <td><%= scrobble.artist.name %></td>
      <td><%= scrobble.track.name %></td>
      <td><%= scrobble.album.name %></td>
    </tr>
  <% end %>
  </tbody>
</table>
