<h1>Scrobbles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Played At</th>
      <th>Artist</th>
      <th>Track</th>
    </tr>
  </thead>
  <tbody>
  <% @scrobbles.each do |scrobble| %>
    <tr>
      <td><%= link_to_user scrobble.user %></td>
      <td><%= scrobble.listened_at %></td>
      <td><%= link_to_artist scrobble.artist %></td>
      <td><%= link_to_track scrobble.track %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= will_paginate @scrobbles %>
